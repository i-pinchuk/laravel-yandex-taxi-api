{
    "name": "ipinchuk/laravel-yandex-taxi-api",
    "description": "Laravel wrapper package for the Yandex.Taxi API",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel","yandex", "taxi", "api", "Яндекс.Такси"],
    "authors": [
        {
            "name": "Igor Pinchuk",
            "email": "ipinchuk.developer@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "ipinchuk/php-yandex-taxi-api": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IPinchuk\\LaravelYandexTaxi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IPinchuk\\LaravelYandexTaxi\\YandexTaxiServiceProvider"
            ],
            "aliases": {
                "YandexTaxiClient": "IPinchuk\\LaravelYandexTaxi\\Facades\\YandexTaxiClient"
            }
        }
    }
}
